{
    "name": "denisyilmaz/oembed",
    "description": "Kirby 3 oembed Field",
    "version": "0.0.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "autoload": {
        "files": [
            "fields/oembed.php"
        ]
    },
    "authors": [
        {
            "name": "Denis Yilmaz",
            "email": "mail@denisyilmaz.de"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.1"
    },
    "require-dev": {},
    "scripts": {
        "build": "composer update --ignore-platform-reqs; composer dumpautoload -o;",
        "zip": "rm kirby-oembed.zip; composer update --ignore-platform-reqs; composer dumpautoload -o; zip -r kirby-oembed.zip . -x *.git*; composer dumpautoload -o;"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "beta"
}
